{
  "name": "esn-map-server",
  "version": "1.0.0",
  "description": "projet de formation",
  "main": "index.js",
  "author": "loicbdev",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "test:ci": "npm run test -- --clearCache --ci --watchAll=false",
    "start:dev": "nodemon",
    "start": "npm run build && node build/index.js",
    "build": "rimraf ./build && tsc",
    "security:audit": "npm audit && snyk test",
    "security-gates": "ts-node scripts/security-gates.ts",
    "lint": "eslint . --ext .ts --fix",
    "prettier": "prettier --config .prettierrc \"src/**/*.ts\" --write",
    "precommit": "lint-staged",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli",
    "migration:create-dev": "typeorm-ts-node-commonjs migration:create src/database/migrations/map",
    "migration:generate-dev": "typeorm-ts-node-commonjs migration:generate -d src/config/typeorm/data-source.ts src/database/migrations/map",
    "migration:run-dev": "typeorm-ts-node-commonjs migration:run -d src/config/typeorm/data-source.ts"
  },
  "lint-staged": {
    "src/**/*.ts*": [
      "npm run lint",
      "npm run prettier"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "dependencies": {
    "axios": "^1.7.9",
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.2.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "password-validator": "^5.3.0",
    "pg": "^8.11.5",
    "reflect-metadata": "^0.2.2",
    "swagger-jsdoc": "5.0.1",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.20",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.12.8",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/validator": "^13.11.9",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-security": "^3.0.1",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "snyk": "^1.1294.3",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  }
}
